<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
    <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../../data/images/envi2.png" />
         </image>
      </ImageView>
      <Pane fx:id="endGameFrame" layoutX="176.0" layoutY="62.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="250.0" />
        <Button fx:id="newGame" layoutX="101.0" layoutY="192.0" mnemonicParsing="false" onMouseClicked="#loadNewGame" prefHeight="50.0" prefWidth="150.0" text="New Game" />
        <Button fx:id="quit" layoutX="351.0" layoutY="192.0" mnemonicParsing="false" onMouseClicked="#quitGame" prefHeight="50.0" prefWidth="150.0" text="Quit" />
        <Label fx:id="endGameLb" layoutX="206.0" layoutY="90.0" text="End Game">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
      <Text fx:id="winner" layoutX="60.0" layoutY="288.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Winner Text" textAlignment="CENTER" wrappingWidth="480.0">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
